	</plot>
	
	<plot name="hwt_prob_brr03" inherits="hwt_prob_brr" run="false">
		<tmpl>
			<data_file>{vx_mask}/grid_stat/lead_series_init/{fcst_init_beg}/PROB_APCP_03_BRR_{obs_thresh?symbol=letters}.data</data_file>
