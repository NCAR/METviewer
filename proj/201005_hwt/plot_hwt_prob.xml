			<title>{vx_mask} APCP_03 {dep1_1_stat1} {obs_thresh?units=in}\" - CONST INIT TIME {fcst_init_beg}</title>
			<x_label>FCST_LEAD</x_label>
			<y1_label>APCP_03 BRIER SCORE, REL., RES., UNC.</y1_label>
		</tmpl>
	</plot>
	
